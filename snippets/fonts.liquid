{% # theme-check-disable %}
  <link
    rel="preload"
    as="font"
    href="{{ 'ABCDiatype-Medium-Trial.woff2' | asset_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
  <link
    rel="preload"
    as="font"
    href="{{ 'ABCDiatype-Medium-Trial.woff2' | asset_url }}"
    type="font/woff"
    crossorigin
    fetchpriority="low"
  >
  <link
    rel="preload"
    as="font"
    href="{{ 'ABCDiatype-Regular-Trial.woff2' | asset_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
  <link
    rel="preload"
    as="font"
    href="{{ 'ABCDiatype-Regular-Trial.woff' | asset_url }}"
    type="font/woff"
    crossorigin
    fetchpriority="low"
  >
  {%- unless settings.type_accent_font.system? -%}
    <link
      rel="preload"
      as="font"
      href="{{ settings.type_accent_font | font_url }}"
      type="font/woff2"
      crossorigin
      fetchpriority="low"
    >
  {%- endunless -%}
{% # theme-check-enable %}
